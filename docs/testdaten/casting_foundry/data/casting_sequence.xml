<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_casting_order" model="ir.sequence">
            <field name="name">Gießauftrag</field>
            <field name="code">casting.order</field>
            <field name="prefix">GA-%(year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_casting_quality_check" model="ir.sequence">
            <field name="name">Qualitätsprüfung</field>
            <field name="code">casting.quality.check</field>
            <field name="prefix">QC-%(year)s-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">SCM Bestellung</field>
            <field name="code">scm.purchase.order</field>
            <field name="prefix">PO-%(year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_production_order" model="ir.sequence">
            <field name="name">SCM Fertigungsauftrag</field>
            <field name="code">scm.production.order</field>
            <field name="prefix">FA-%(year)s-</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_delivery" model="ir.sequence">
            <field name="name">SCM Lieferung</field>
            <field name="code">scm.delivery</field>
            <field name="prefix">LS-%(year)s-</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
